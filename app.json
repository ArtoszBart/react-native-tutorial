{
  "expo": {
    "name": "react-native-tutorial",
    "slug": "react-native-tutorial",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b",
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.artoszbart.react-native-tutorial",
        "buildNumber": "1.0.0"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./app/assets/adaptive-icon.png",
          "backgroundColor": "#FFFFFF"
        },
        "package": "com.artoszbart.react-native-tutorial",
        "versionCode": 1
      },
      "web": {
        "favicon": "./app/assets/favicon.png"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "notification": {
      "iosDisplayInForeground": true
    },
    "extra": {
      "bugsnag": {
        "apiKey": "3eea855703a5c9a110f4f4d9b476eab3"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.artoszbart.react-native-tutorial"
    }
  }
}
